import java.util.Scanner;

public class NestedIfAGE {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);		
		
		System.out.print("Enter Sex Code: ");
		char sexCode = scan.nextLine().toLowerCase().charAt(0);
		
		if(sexCode == 'm') {
			System.out.print("Enter Age: ");
			int age = scan.nextInt();
			
			if(age == 21)
				System.out.print("Debutant!");
			else
				System.out.print("Not Debutant!");
		}
		else if(sexCode == 'f') {
			System.out.print("Enter Age: ");
			int age = scan.nextInt();
			
			if(age == 18)
				System.out.print("Debutant!");
			else
				System.out.print("Not Debutant!");
		}		
		else
			System.out.print("Invalid Sex Code!");
	}
}
