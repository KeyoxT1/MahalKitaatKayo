import java.util.Scanner;

class AreaOfSquare {
    public static void main(String[] args) {
    	
        Scanner s = new Scanner(System.in);
    	
    	int a;
    	double area;
    	System.out.print("Enter Area of Square: ");
        a = s.nextInt();
        area = (a * a);
        System.out.println("The Area of Square is: " + area );
    }
}

